  "scripts": {
    "start": "cross-env PORT=3003 react-scripts start",
    "start:frontend": "cross-env PORT=3003 react-scripts start",
    "start-win": "set PORT=3003 && react-scripts start",
    "dev": "npm run start:frontend",
    "dev-win": "npm run start-win",
    "dev:backend": "node server.js",
    "dev:backend:watch": "nodemon server.js",
    "dev:full": "concurrently -n frontend,backend -c blue,green \"npm run start:frontend\" \"npm run dev:backend:watch\"",
    "dev:full-win": "concurrently -n frontend,backend -c blue,green \"npm run start-win\" \"npm run dev:backend\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:all": "npm install && npm install express mongoose cors dotenv bcryptjs jsonwebtoken nodemon"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "concurrently": "^8.2.2",
